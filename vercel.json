{
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/login/(.*)",
      "dest": "/login/$1"
    },
    {
      "src": "/create-an-addon/(.*)",
      "dest": "/create-an-addon/$1"
    },
    {
      "src": "/uploads/(.*)",
      "dest": "/uploads/$1"
    }
  ]
}
